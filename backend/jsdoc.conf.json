{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": [
      "jsdoc",
      "closure"
    ]
  },
  "source": {
    "include": [
      "."
    ],
    "exclude": [
      ".meteor",
      "node_modules"
    ],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "opts": {
    "destination": "../docs/api/backend",
    "recurse": true,
    "readme": "./README.md",
    "template": "node_modules/docdash"
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "docdash": {
    "static": [
      false
    ],
    "sort": [
      true
    ],
    "sectionOrder": [
      "Classes",
      "Modules",
      "Externals",
      "Events",
      "Namespaces",
      "Mixins",
      "Tutorials",
      "Interfaces"
    ],
    "disqus": "",
    "openGraph": {
      "title": "",
      "type": "website",
      "image": "",
      "site_name": "",
      "url": ""
    },
    "meta": {
      "title": "",
      "description": "",
      "keyword": ""
    },
    "search": [
      true
    ],
    "commonNav": [
      true
    ],
    "collapse": [
      true
    ],
    "wrap": [
      true
    ],
    "typedefs": [
      false
    ],
    "navLevel": [
      0
    ],
    "private": [
      false
    ],
    "removeQuotes": [
      "trim"
    ],
    "scripts": [],
    "menu": {
      "Project Website": {
        "href": "https://github.com/leaonline/leaonline-app",
        "target": "_blank",
        "class": "menu-item",
        "id": "website_link"
      }
    },
    "scopeInOutputPath": [
      true
    ],
    "nameInOutputPath": [
      true
    ],
    "versionInOutputPath": [
      true
    ]
  }
}