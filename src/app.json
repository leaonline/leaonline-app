{
  "expo": {
    "name": "lea.App",
    "slug": "leaonline",
    "version": "1.0.1",
    "orientation": "portrait",
    "privacy": "public",
    "jsEngine": "hermes",
    "icon": "./lib/assets/icons/favicon-1024.png",
    "splash": {
      "image": "./lib/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "packagerOpts": {
      "assetExts": ["md"]
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "de.uni-bremen.leaonline",
      "infoPlist": {
        "UIViewControllerBasedStatusBarAppearance": "YES"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./lib/assets/icons/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.testCompany.leaonline"
    }
  }
}
