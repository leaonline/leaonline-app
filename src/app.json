{
  "expo": {
    "name": "lea.App",
    "slug": "leaonline",
    "version": "1.2.0",
    "orientation": "portrait",
    "privacy": "public",
    "jsEngine": "hermes",
    "icon": "./lib/assets/icons/favicon-1024.png",
    "splash": {
      "image": "./lib/assets/splash_screen.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "packagerOpts": {
      "assetExts": [
        "md"
      ]
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "de.uni-bremen.leaonline.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./lib/assets/icons/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.testCompany.leaonline",
      "versionCode": 7,
      "allowBackup": false
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "minSdkVersion": 21,
            "targetSdkVersion": 34,
            "compileSdkVersion": 34,
            "newArchEnabled": false
          },
          "ios": {
            "deploymentTarget": "14.0"
          }
        }
      ],
      "expo-font",
      "expo-secure-store"
    ]
  }
}
