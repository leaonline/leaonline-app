{
  "scripts": {
    "start": "expo start --dev-client",
    "prod": "expo start --no-dev --minify",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "lint": "standardx ./lib --env jest --env mocha | snazzy",
    "lint:fix": "standardx ./lib --fix --env jest --env mocha | snazzy",
    "test": "node_modules/.bin/jest --coverage",
    "docs": "jsdoc -c jsdoc.conf.json",
    "build:ios": "react-native bundle --entry-file='index.js' --bundle-output='./ios/main.jsbundle' --dev=false --platform='ios'"
  },
  "dependencies": {
    "@babel/preset-react": "^7.16.0",
    "@expo/metro-config": "~0.18.11",
    "@meteorrn/core": "^2.8.2-rc.1",
    "@meteorrn/minimongo": "^1.0.2",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/netinfo": "11.3.1",
    "@react-navigation/native": "^6.1.7",
    "@react-navigation/native-stack": "^6.9.1",
    "expo": "^51.0.0",
    "expo-av": "~14.0.7",
    "expo-build-properties": "~0.12.5",
    "expo-constants": "~16.0.2",
    "expo-dev-client": "~4.0.23",
    "expo-font": "~12.0.9",
    "expo-keep-awake": "~13.0.2",
    "expo-secure-store": "~13.0.2",
    "expo-speech": "~12.0.2",
    "expo-splash-screen": "~0.27.5",
    "expo-status-bar": "~1.12.1",
    "i18next": "^21.5.4",
    "marked": "4.2.12",
    "next-frame": "0.2.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "^11.14.3",
    "react-native": "0.74.5",
    "react-native-elements": "^3.4.2",
    "react-native-markdown-display": "^7.0.0-alpha.2",
    "react-native-restart": "0.0.27",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "3.31.1",
    "react-native-svg": "15.2.0",
    "react-native-vector-icons": "^9.0.0",
    "react-native-walkthrough-tooltip": "^1.5.0",
    "simpl-schema": "^3.4.1",
    "string.prototype.matchall": "^4.0.6",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/preset-env": "^7.16.11",
    "@babel/register": "^7.17.0",
    "@testing-library/react-native": "^8.0.0",
    "@types/react-test-renderer": "^17.0.1",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native-a11y": "^3.3.0",
    "jest": "^29.2.1",
    "jest-expo": "~51.0.4",
    "jsdoc": "^4.0.2",
    "react-test-renderer": "^18.0.0",
    "sinon": "^14.0.0",
    "snazzy": "^9.0.0",
    "standardx": "^7.0.0"
  },
  "eslintConfig": {
    "parser": "@babel/eslint-parser",
    "parserOptions": {
      "sourceType": "module",
      "allowImportExportEverywhere": false
    },
    "extends": [
      "plugin:react-native-a11y/android"
    ],
    "plugins": [
      "import",
      "react-hooks"
    ],
    "rules": {
      "react-hooks/rules-of-hooks": "error",
      "react-hooks/exhaustive-deps": "off",
      "import/no-default-export": "error",
      "brace-style": [
        "error",
        "stroustrup",
        {
          "allowSingleLine": true
        }
      ],
      "no-console": [
        "error",
        {
          "allow": [
            "warn",
            "error"
          ]
        }
      ]
    }
  },
  "private": true,
  "name": "src",
  "version": "1.1.1"
}
