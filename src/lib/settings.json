{
  "isDevelopment": false,
  "isDeveloperRelease": true,
  "backend": {
    "url": "wss://appbackend.lealernen.de/websocket",
    "maxTimeout": 3000,
    "interval": 250,
    "methods": {
      "defaultTimeout": 5000,
      "users": {
        "create": "users.methods.create",
        "delete": "users.methods.delete"
      },
      "content": {
        "map": "content.methods.map",
        "unit": "content.methods.unit",
        "session": "content.methods.session"
      },
      "progress": {
        "get": "progress.methods.get"
      },
      "session": {
        "update": "session.methods.update"
      },
      "response": {
        "submit": "response.methods.submit"
      },
      "terms": {
        "get": {
          "name": "legal.methods.get",
          "args": {
            "name": "terms"
          }
        }
      },
      "dev": {
        "get": "devData.methods.get"
      }
    }
  },
  "content": {
    "url": "https://content.lealernen.de"
  },
  "log": {
    "level": 0,
    "target": {
      "active": false,
      "transport": "ddp",
      "method": "analytics.methods.send",
      "batchSize": 1,
      "level": 4,
      "separator": "~"
    }
  },
  "debug": {
    "layoutBorders": false,
    "state": false,
    "sync": false,
    "home": false,
    "map": false,
    "unit": false,
    "tts": false,
    "accounts": false,
    "AppSession": false
  }
}
